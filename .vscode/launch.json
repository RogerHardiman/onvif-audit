    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/onvif-audit.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program with Filename",
            "program": "${workspaceFolder}/onvif-audit.js",
            "args": ["--filename","./camera_list.json"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program with IP Range",
            "program": "${workspaceFolder}/onvif-audit.js",
            "args": ["--ip","192.168.1.1-192.168.1.5"]
        }
    ]
}